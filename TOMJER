//https://www.codechef.com/problems/TOMJER/

#include<stdio.h>
#include<math.h>
#define MOD 1000
int main()
{
    int t;
    scanf("%d",&t);
    while(t--)
    {
        int n;
        unsigned long long sum=0;
        scanf("%d",&n);
        sum=pow(2,n-1);
        sum=sum%MOD;
        printf("%llu\n",sum);
    }
    return 0;
}
 
